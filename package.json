{
  "name": "applicationstyleguide",
  "version": "1.1.1",
  "description": "Seed App featuring Angular 4 (Router, Http, Forms, Services, Tests, E2E, Coverage), Karma, Protractor, Jasmine, Istanbul, TypeScript, and Webpack",
  "keywords": [
    "angular4",
    "webpack3",
    "typescript"
  ],
  "author": "SimplyWell,Inc",
  "homepage": "tbd",
  "license": "MIT",
  "scripts": {
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "ci:aot": "npm run lint && npm run test && npm run build:aot && npm run e2e",
    "ci:jit": "npm run lint && npm run test && npm run build:prod && npm run e2e",
    "ci:nobuild": "npm run lint && npm test && npm run e2e",
    "ci:testall": "npm run lint && npm run test && npm run build:prod && npm run e2e && npm run build:aot && npm run e2e",
    "ci:travis": "npm run lint && npm run test && npm run build:dev && npm run e2e:travis && npm run build:prod && npm run e2e:travis && npm run build:aot && npm run e2e:travis",
    "ci": "npm run ci:testall",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postinstall": "npm run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run rimraf package-lock.json && npm run server:dev",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "precommit": "npm run lint",
    "build:ui-button": "ng-packagr -p src/app/libs/ui-button/package.json",
    "build:ui-checkbox": "ng-packagr -p src/app/libs/ui-checkbox/package.json",
    "build:ui-datatable": "ng-packagr -p src/app/libs/ui-datatable/package.json",
    "build:ui-datepicker": "ng-packagr -p src/app/libs/ui-datepicker/package.json",
    "build:ui-form-select": "ng-packagr -p src/app/libs/ui-form-select/package.json",
    "build:ui-fullscreen-modal": "ng-packagr -p src/app/libs/ui-fullscreen-modal/package.json",
    "build:ui-icon": "ng-packagr -p src/app/libs/ui-icon/package.json",
    "build:ui-input": "ng-packagr -p src/app/libs/ui-input/package.json",
    "build:ui-loader": "ng-packagr -p src/app/libs/ui-loader/pacakge.json",
    "build:ui-modal": "ng-packagr -p src/app/libs/ui-modal/package.json",
    "build:ui-overflow-menu": "ng-packagr -p src/app/libs/ui-overflow-menu/package.json",
    "build:ui-panel": "ng-packagr -p src/app/libs/ui-panel/package.json",
    "build:ui-radiobutton": "ng-packagr -p src/app/libs/ui-radiobutton/package.json",
    "build:ui-selector": "ng-packagr -p src/app/libs/ui-selector/package.json",
    "build:ui-tag": "ng-packagr -p src/app/libs/ui-tag/package.json",
    "build:ui-toast": "ng-packagr -p src/app/libs/ui-toast/package.json",
    "build:ui-tooltip": "ng-packagr -p src/app/libs/ui-tooltip/package.json",
    "build:ui-textarea": "ng-packagr -p src/app/libs/ui-textarea/package.json",
    "build:xui-test": "ng-packagr -p src/app/libs/xui-test/package.json",
    "packagr": "ng-packagr -p ng-package.json",
    "publish:ui-button": "npm run build:ui-button && npm publish src/app/libs/ui-button/dist",
    "publish:ui-checkbox": "npm run build:ui-checkbox && npm publish src/app/libs/ui-checkbox/dist",
    "publish:ui-datatable": "npm run build:ui-datatable && npm publish src/app/libs/ui-datatable/dist",
    "publish:ui-datepicker": "npm run build:ui-datepicker && npm publish src/app/libs/ui-datepicker/dist",
    "publish:ui-form-select": "npm run build:ui-form-select && npm publish src/app/libs/ui-form-select/dist",
    "publish:ui-fullscreen-modal": "npm run build:ui-fullscreen-modal && npm publish src/app/libs/ui-fullscreen-modal/dist",
    "publish:ui-icon": "npm run build:ui-icon && npm publish src/app/libs/ui-icon/dist",
    "publish:ui-input": "npm run build:ui-input && npm publish src/app/libs/ui-input/dist",
    "publish:ui-loader": "npm run build:ui-loader && npm publish src/app/libs/ui-loader/pacakge.json",
    "publish:ui-modal": "npm run build:ui-modal && npm publish src/app/libs/ui-modal/dist",
    "publish:ui-overflow-menu": "npm run build:ui-overflow-menu && npm publish src/app/libs/ui-overflow-menu/dist",
    "publish:ui-panel": "npm run build:ui-panel && npm publish src/app/libs/ui-panel/dist",
    "publish:ui-radiobutton": "npm run build:ui-radiobutton && npm publish src/app/libs/ui-radiobutton/dist",
    "publish:ui-selector": "npm run build:ui-selector && npm publish src/app/libs/ui-selector/dist",
    "publish:ui-tag": "npm run build:ui-tag && npm publish src/app/libs/ui-tag/dist",
    "publish:ui-toast": "npm run build:ui-toast && npm publish src/app/libs/ui-toast/dist",
    "publish:ui-tooltip": "npm run build:ui-tooltip && npm publish src/app/libs/ui-tooltip/dist",
    "publish:ui-textarea": "npm run build:ui-textarea && npm publish src/app/libs/ui-textarea/dist",
    "publish:xui-test": "npm run build:ui-tooltip && npm publish src/app/libs/xui-test/dist",
    "test:install:ui-datepicker": "npm remove @simplywell/ui-datepicker && npm i -D @simplywell/ui-datepicker@latest"
  },
